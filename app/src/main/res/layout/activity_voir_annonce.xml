<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.a21713885.l3.unicaen.android.annonceapp.VoirAnnonceActivity">


    <TextView
        android:id="@+id/mail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/contacter"
        android:layout_alignStart="@+id/contacter"
        android:layout_below="@+id/contacter"
        android:text="textview"
        android:textSize="20dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/telephone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/mail"
        android:layout_alignStart="@+id/mail"
        android:layout_below="@+id/mail"
        android:text="textview"
        android:textSize="20dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textview"
        android:layout_width="80dp"
        android:layout_height="35dp"
        android:layout_alignBaseline="@+id/prix5"
        android:layout_alignBottom="@+id/prix5"
        android:layout_alignLeft="@+id/description"
        android:layout_alignStart="@+id/description"
        android:text="PubliÃ© le " />

    <TextView
        android:id="@+id/prix5"
        android:layout_width="220dp"
        android:layout_height="35dp"
        android:layout_below="@+id/description"
        android:layout_marginTop="12dp"
        android:layout_toEndOf="@+id/textview"
        android:layout_toRightOf="@+id/textview"
        android:text="TextView" />

    <TextView
        android:id="@+id/description"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/prix"
        android:layout_alignStart="@+id/prix"
        android:layout_below="@+id/prix"
        android:layout_marginTop="11dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/codePostal"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:layout_alignBaseline="@+id/prix"
        android:layout_alignBottom="@+id/prix"
        android:layout_marginLeft="17dp"
        android:layout_marginStart="17dp"
        android:layout_toEndOf="@+id/prix"
        android:layout_toRightOf="@+id/prix"
        android:text="TextView" />

    <TextView
        android:id="@+id/ville"
        android:layout_width="110dp"
        android:layout_height="40dp"
        android:layout_alignBaseline="@+id/codePostal"
        android:layout_alignBottom="@+id/codePostal"
        android:layout_marginLeft="13dp"
        android:layout_marginStart="13dp"
        android:layout_toEndOf="@+id/codePostal"
        android:layout_toRightOf="@+id/codePostal"
        android:text="TextView" />

    <TextView
        android:id="@+id/Titre"
        android:layout_width="300dp"
        android:layout_height="40dp"
        android:layout_alignLeft="@+id/imageView"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/imageView"
        android:layout_marginTop="18dp"
        android:text="Vends Ordinateur HP"
        android:textSize="24dp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="300dp"
        android:layout_height="200dp"
        android:layout_below="@+id/Titre"
        android:layout_centerHorizontal="true"
        app:srcCompat="@mipmap/ic_launcher" />

    <TextView
        android:id="@+id/prix"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:layout_alignLeft="@+id/imageView"
        android:layout_alignStart="@+id/imageView"
        android:layout_below="@+id/imageView"
        android:layout_marginTop="18dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/nom"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_alignEnd="@+id/prix5"
        android:layout_alignRight="@+id/prix5"
        android:layout_below="@+id/prix5"
        android:text="textview"
        android:textSize="20dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/contacter"
        android:layout_width="90dp"
        android:layout_height="40dp"
        android:layout_alignLeft="@+id/textview"
        android:layout_alignStart="@+id/textview"
        android:layout_below="@+id/textview"
        android:text="Contacter"
        android:textSize="20dp"
        android:textStyle="bold" />


</RelativeLayout>
